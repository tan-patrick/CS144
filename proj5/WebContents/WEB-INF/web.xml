<web-app id="simple" version="2.4">
    <servlet>
        <servlet-name>SearchServlet</servlet-name>
        <servlet-class>edu.ucla.cs.cs144.SearchServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SearchServlet</servlet-name>
        <url-pattern>/search</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>ItemServlet</servlet-name>
        <servlet-class>edu.ucla.cs.cs144.ItemServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ItemServlet</servlet-name>
        <url-pattern>/item</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>ProxyServlet</servlet-name>
        <servlet-class>edu.ucla.cs.cs144.ProxyServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ProxyServlet</servlet-name>
        <url-pattern>/suggest</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>InputCardServlet</servlet-name>
        <servlet-class>edu.ucla.cs.cs144.InputCardServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>InputCardServlet</servlet-name>
        <url-pattern>/inputCard</url-pattern>
    </servlet-mapping>
        <servlet>
        <servlet-name>ConfirmationServlet</servlet-name>
        <servlet-class>edu.ucla.cs.cs144.ConfirmationServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ConfirmationServlet</servlet-name>
        <url-pattern>/confirmation</url-pattern>
    </servlet-mapping>
		
		
	
</web-app>
